{
  "name": "ava-portfolio",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "nm-watch": "nodemon ./bin/www",
    "sass-watch": "sass --watch ./public/stylesheets/scss/main.scss:./public/stylesheets/css/main.css",
    "dev": "concurrently --kill-others \"npm run nm-watch \" \"npm run sass-watch\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "hbs": "~4.0.1",
    "http-errors": "~1.6.2",
    "mongoose": "^5.4.17",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
